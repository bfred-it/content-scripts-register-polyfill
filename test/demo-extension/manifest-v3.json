{
	"name": "content-scripts-register-polyfill-mv3",
	"version": "0.0.0",
	"manifest_version": 3,
	"permissions": ["webNavigation", "scripting"],
	"host_permissions": [
		"https://ephiframe.vercel.app/*",
		"https://iframe-test-page-n786423ca-fregante.vercel.app/*",
		"https://fregante.github.io/*"
	],
	"background": {
		"service_worker": "/background.js"
	},
	"content_scripts": [
		{
			"all_frames": true,
			"matches": ["https://ephiframe.vercel.app/*"],
			"js": ["/static.js"],
			"css": ["/static.css"]
		},
		{
			"all_frames": true,
			"matches": ["https://iframe-test-page-n786423ca-fregante.vercel.app/*"],
			"js": ["/static.js"],
			"css": ["/static.css"]
		},
		{
			"all_frames": true,
			"matches": ["https://fregante.github.io/pixiebrix-testing-ground/*"],
			"exclude_matches": [
				"https://fregante.github.io/pixiebrix-testing-ground/Parent-page*"
			],
			"js": ["/static.js"],
			"css": ["/static.css"]
		}
	]
}
